<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/settings_title">

        <SwitchPreference
            android:key="NYANG?"
            android:title="NYANG!"/>

        <PreferenceScreen
            android:key="keyword_screen"
            android:title="User Preferences"
            android:summary="Don't use">

            <SwitchPreference
                android:defaultValue="false"
                android:key="keyword"
                android:summary="NYANG"
                android:title="keyword notification"/>

            <ListPreference
                android:entries="@array/array_my_profile"
                android:entryValues="@array/array_my_profile"
                android:key="first_array"
                android:summary="My profile: First array of user preferences"
                android:title="First array of user preferences"
                android:dependency="keyword"/>

        </PreferenceScreen>

        <SwitchPreference
            android:key="sound"
            android:title="SOUUUUND"/>

        <ListPreference
            android:entries="@array/array_my_profile"
            android:entryValues="@array/array_my_profile"
            android:key="second_array"
            android:summary="My profile: Second array of user preferences"
            android:title="Second array of user preferences"/>

    </PreferenceCategory>

</PreferenceScreen>